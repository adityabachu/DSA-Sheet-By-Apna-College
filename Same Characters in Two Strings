class Solution 
{ 
    int sameChar(String A, String B) 
    {
        // code here
        int count = 0;
        A = A.toLowerCase();
        B = B.toLowerCase();
        for(int i = 0; i < A.length(); i++)
        {
            if(A.charAt(i) == B.charAt(i))
            {
                count++;
            }
        }
        return count;
    }
} 
